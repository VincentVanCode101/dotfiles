#!/usr/bin/env bash

find . -maxdepth 1 -type f ! \( -iname "*.pdf" -o -iname "*.jpg" -o -iname "*.jpeg" -o -iname "*.png" -o -iname "*.gif" -o -iname "*.zip" -o -iname "*.tar" -o -iname "*.gz" \) -print0 | while IFS= read -r -d '' file; do
    echo "${file##./}:"
    cat "$file"
    echo -e "\n"
done | copy
